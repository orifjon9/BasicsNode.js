<section class="pagination">
    <%if(hasPreviousPage){%>
        <a href="?page=<%=(currentPage - 1)%>"><%=currentPage - 1%></a>
    <%}%>
    <a href="?page=<%=currentPage%>" class="active"><%=currentPage%></a>
    <%if(hasNextPage){%>
        <a href="?page=<%=(currentPage + 1)%>"><%=currentPage+1%></a>
    <%}%>
    <%if(hasNextPage && (currentPage + 1) !== lastPage){%>
        <a href="?page=<%=lastPage%>">Last</a>
    <%}%>
</section>